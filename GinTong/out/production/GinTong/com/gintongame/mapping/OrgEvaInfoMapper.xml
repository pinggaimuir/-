<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gintongame.mapping.OrgEvaInfoMapper">
    <insert id="insertSingle" parameterType="JavaBean.OrgEvaInfo">
        insert into org_eva_info (evaluator,etype,tag,detail,etime,eplace,eback,job_interview,uuid)
        VALUES
        (#{evaluator},#{etype},#{tag},#{detail},#{etime},#{eplace},#{eback},#{job_interview},#{uuid})
    </insert>
</mapper>