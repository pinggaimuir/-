<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gintongame.mapping.BasBusinessInfoMapper">
    <insert id="insertPerSkillInfo" parameterType="JavaBean.PerSkillInfo" useGeneratedKeys="true">
        insert into per_skill_info
        (manage,specialty,other,uuid)
        VALUES
        (#{manage},#{specialty},#{other},#{uuid})
    </insert>
</mapper>