<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gintongame.mapping.OrgProductMapper">

    <insert id="insertOPDuct" parameterType="JavaBean.OrgProduct" useGeneratedKeys="true" >
        insert into org_product
        (oname,pname,rtype,rgrade,rg_desc,ouuid,pr_uuid,source)
        values
        (#{oname},#{pname},#{rtype},#{rgrade},#{rg_desc},#{ouuid},#{pr_uuid},#{source})
    </insert>

</mapper>