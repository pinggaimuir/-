<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gintongame.mapping.PerEducationInfoMapper">

    <insert id="insertPerEducationInfo" parameterType="JavaBean.PerEducationInfo" useGeneratedKeys="true">
        insert into per_education_info(uuid,school,major,diploma,start_date,end_date,event,type,place) values(
             #{uuid},#{school},#{major},#{diploma},#{start_date},#{end_date},#{event},#{type},#{place}
        );
    </insert>

</mapper>